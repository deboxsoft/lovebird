{
  "extends": "../tsconfig.webapp",
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "",
    "paths": {
      "*": [
        "../node_modules/@deboxsoft/component-webapp-react/__types/*",
        "./*"
      ],
      "react/*": [
        "../node_modules/react/*"
      ],
      "react-dom/*": [
        "../node_modules/react-dom/*"
      ],
      "styled-components/*": [
        "../node_modules/styled-components/*",
        "../node_modules/@deboxsoft/component-webapp-react/__types/styled-components/*"
      ],
      "navi/*": [
        "../node_modules/navi/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}

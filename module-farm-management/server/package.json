{
  "name": "@deboxsoft/lb-module-farm-management-server",
  "version": "0.0.3",
  "description": "module manajemen farm lovebird",
  "main": "libs",
  "types": "libs",
  "module": "libs",
  "scripts": {
    "lint": "npm run eslint --fix",
    "check": "npm run lint && tsc --noEmit",
    "clean": "rimraf libs",
    "build": "npm run clean && tsc",
    "build:typings": "tsc -p tsconfig.typings.json",
    "build:dev": "npm run clean && tsc --watch",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "test": "cross-env NODE_ENV=test jest src",
    "test:watch": "npm run test --watch",
    "test:coverage": "npm run test --coverage",
    "typeorm": "ts-node ./node_modules/@deboxsoft/typeorm/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/deboxsoft/lovebird"
  },
  "keywords": [
    "graphql",
    "api",
    "service"
  ],
  "author": "Nurdiansyah",
  "dependencies": {
    "@deboxsoft/config": "^1.0.5",
    "@deboxsoft/graphql": "^0.0.29",
    "@deboxsoft/lb-module-farm-management-types": "^0.0.1",
    "@deboxsoft/logger": "^1.0.1",
    "@deboxsoft/typeorm": "^0.2.21-beta.7",
    "@deboxsoft/user-model": "^0.0.1",
    "config": "^3.1.0",
    "dataloader": "^1.4.0",
    "fast-memoize": "^2.5.1",
    "lodash": "^4.17.15",
    "lodash.merge": "^4.6.1",
    "mariadb": "^2.0.3",
    "moment": "^2.24.0",
    "nanoid": "^2.0.1"
  },
  "devDependencies": {
    "@types/config": "^0.0.34",
    "lint-staged": "^8.1.5",
    "sqlite3": "^4.0.6"
  },
  "standard-version": {
    "releaseCommitMessageFormat": "chore(release): lb-module-management-server:{{currentTag}}",
    "scripts": {
      "postbump": "npm publish"
    },
    "skip": {
      "changelog": true,
      "tag": true
    }
  }
}
